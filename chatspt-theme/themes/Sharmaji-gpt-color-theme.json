{
    "name": "Sharmaji-gpt",
    "type": "dark",
    "colors": {
        "editor.background": "#1E1E1E",
        "editor.foreground": "#D4D4D4",
        "editorCursor.foreground": "#D4D4D4",
        "editor.lineHighlightBackground": "#2A2A2A",
        "editorLineNumber.foreground": "#5A5A5A",
        "editor.selectionBackground": "#007ACC",
        // "editor.inactiveSelectionBackground": "#3C3C3C",
        "editorWhitespace.foreground": "#FFFFFF",
        // "editorIndentGuide.background": "#555555"
    },
    "tokenColors": [
        {
            "scope": [
                "keyword",
                "variable.language",
                "variable.other"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "scope": [
                "string",
                "constant.other"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "scope": [
                "variable",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "scope": [
                "function",
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "scope": [
                "comment",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#6A9955",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#F44747"
            }
        },
        {
            "scope": [
                "meta.warning"
            ],
            "settings": {
                "foreground": "#FFA500"
            }
        }
    ]
}
